<%%
// <%= name %>/_<%= name %>-preview.ejs
// A preview HTML of "<%= name %>"
  this.name = '<%= name %>'; const name = this.name;
  this.ejsRoot = '../../'; const ejsRoot = this.ejsRoot;
  if (typeof util === 'undefined') { include(ejsRoot + 'util/_util'); util = getUtil(); }
  if (typeof data === 'undefined') { include(ejsRoot + 'data/_data'); data = getData(); }
<%- end %>%>
<%%- include(ejsRoot + 'components/<%= name %>/_<%= name %>', {data:data}); <%- end %>%>
